### PHYSICAL ACTIVITY GOAL REVIEW - AVAILABLE FOR ALL PATIENTS AFTER SESSION 3
begin physicalactivitygoalreview
show pagoalreview if (and (paplan = "mix", session > 3))  ### PA goal review for those who chose the mix of PA plan
set pagoalreview.lastweekgoal1 to loadvalue (username, "currentpagoal1") 
set pagoalreview.lastweekgoal2 to loadvalue (username, "currentpagoal2")
set pagoalreview.lastweekgoal3 to loadvalue (username, "currentpagoal3")
set gl1 to 1
set gl2 to 1
set gl3 to 1
if or (pagoalreview.goal1 = "everyday1", pagoalreview.goal1 = "mostdays1") set gl1 to 2 
if or (pagoalreview.goal2 = "everyday1", pagoalreview.goal2 = "mostdays1") set gl2 to 2 
if or (pagoalreview.goal3 = "everyday1", pagoalreview.goal3 = "mostdays1") set gl3 to 2
set pareviewscore to sum (gl1, gl2, gl3)
set pag1 to 0
if (pagoalreview.goal1 = "everyday1") set pag1 to 3
if (pagoalreview.goal1 = "mostdays1") set pag1 to 2
if (pagoalreview.goal1 = "somedays1") set pag1 to 1
set pag2 to 0
if (pagoalreview.goal2 = "everyday1") set pag1 to 3
if (pagoalreview.goal2 = "mostdays1") set pag1 to 2
if (pagoalreview.goal2 = "somedays1") set pag1 to 1
set pag3 to 0
if (pagoalreview.goal3 = "everyday1") set pag1 to 3
if (pagoalreview.goal3 = "mostdays1") set pag1 to 2
if (pagoalreview.goal3 = "somedays1") set pag1 to 1
### graphing physical activity goal progress to be shown on the nurse pages
graph pag1 to "pagoal1" for username
graph pag2 to "pagoal2" for username
graph pag3 to "pagoal3" for username
after pagoalreview if (pareviewscore > 3) goto mixfeedback1
after pagoalreview if or (pareviewscore = 1, pareviewscore = 2, pareviewscore = 3) goto mixfeedback2

