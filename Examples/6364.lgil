## optional calorie calculator for session 1
show calcalculator1
set decimal to 0
if (calcalculator1.decimal = 0) set decimal to 0
if (calcalculator1.decimal = 1) set decimal to (1/10)
if (calcalculator1.decimal = 2) set decimal to (2/10)
if (calcalculator1.decimal = 3) set decimal to (3/10)
if (calcalculator1.decimal = 4) set decimal to (4/10)
if (calcalculator1.decimal = 5) set decimal to (5/10)
if (calcalculator1.decimal = 6) set decimal to (6/10)
if (calcalculator1.decimal = 7) set decimal to (7/10)
if (calcalculator1.decimal = 8) set decimal to (8/10)
if (calcalculator1.decimal = 9) set decimal to (9/10)

set lb to 0 
if (calcalculator1.pounds = 0) set lb to 0
if (calcalculator1.pounds = 1) set lb to 1
if (calcalculator1.pounds = 2) set lb to 2
if (calcalculator1.pounds = 3) set lb to 3
if (calcalculator1.pounds = 4) set lb to 4
if (calcalculator1.pounds = 5) set lb to 5
if (calcalculator1.pounds = 6) set lb to 6
if (calcalculator1.pounds = 7) set lb to 7
if (calcalculator1.pounds = 8) set lb to 8
if (calcalculator1.pounds = 9) set lb to 9
if (calcalculator1.pounds = 10) set lb to 10
if (calcalculator1.pounds = 11) set lb to 11
if (calcalculator1.pounds = 12) set lb to 12
if (calcalculator1.pounds = 13) set lb to 13

set inch to 0 
if (calcalculator1.inches = 0) set inch to 0
if (calcalculator1.inches = 1) set inch to 1
if (calcalculator1.inches = 2) set inch to 2 
if (calcalculator1.inches = 3) set inch to 3 
if (calcalculator1.inches = 4) set inch to 4 
if (calcalculator1.inches = 5) set inch to 5 
if (calcalculator1.inches = 6) set inch to 6 
if (calcalculator1.inches = 7) set inch to 7 
if (calcalculator1.inches = 8) set inch to 8 
if (calcalculator1.inches = 9) set inch to 9 
if (calcalculator1.inches = 10) set inch to 10 
if (calcalculator1.inches = 11) set inch to 11

if (calcalculator1.stones > 0) set pnds1 to (calcalculator1.stones * 14)
if (calcalculator1.stones > 0) set pnds2 to + (pnds1, lb)
if (pnds2 > 0) set calweight to (pnds2 / 2.2)
if (calcalculator1.Kg > 0) set calweight to + (calcalculator1.Kg, decimal)

if (calcalculator1.feet > 0) set inches1 to (calcalculator1.feet * 12)
if (calcalculator1.feet > 0) set inches2 to + (inches1, inch)
if (inches2 > 0) set height to (inches2 * 2.54)
if (calcalculator1.cm > 0) set height to calcalculator1.cm

set neg to -1
set score1 to +  (10 * calweight , 6.25 * height)
set negscore to (neg * 5)
set score2 to +  (score1 , negscore * calcalculator1.age) 
if (gender = "female") set gendervar to (neg*161)
if (gender = "male") set gendervar to (neg*negscore)
set calorie to +  (score2, gendervar)
if (calcalculator1.activity = "sed") set active to 1.2
if (calcalculator1.activity = "light") set active to 1.375
if (calcalculator1.activity = "mod") set active to 1.550
if (calcalculator1.activity = "very") set active to 1.725
if (calcalculator1.activity = "extra") set active to 1.900
set final to (* (calorie, active))
set loss to (final - 600)
show calcalculator2
set calcalculator2.score to round (final)
set calcalculator2.score2 to round (loss)

